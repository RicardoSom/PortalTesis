<div class="jumbotron">
  <h1>Usuarios ingresados</h1>
</div>


<table class="table table-bordered table-striped">
 <tr>
   <th>name</th>
   <th>lastname</th>
   <th>email</th>
 </tr>
<% @users.each do |user| %>
<tr>
  <td><%= user.name %></td>
  <td><%= user.lastname %></td>
  <td><%= user.email %></td>
  <td><%= link_to "Editar", edit_user_registration_path %></td>
  <td> <%= link_to "Mostrar", usuario_path(user) %></td>
</tr>
<% end %>
</table>
