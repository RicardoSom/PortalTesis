<h1>Estudiantes ingresados</h1>

<table class="table table-bordered table-striped">
 <tr>
   <td>nombre usuario</td>
 </tr>
<% @users.each do |user| %>
<tr>
  <td><%= user.name %></td>
  <td> <%= link_to "Mostrar", user %></td>
  <td><%= link_to "Eliminar", user, method: :delete, data: { confirm: 'Esta seguro?'} %></td>
</tr>
<% end %>
</table>
